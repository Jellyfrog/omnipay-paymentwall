{
    "name": "delatbabel/omnipay-paymentwall",
    "type": "library",
    "description": "PaymentWall driver for the Omnipay payment processing library",
    "keywords": [
        "PaymentWall",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/incube8/omnipay-paymentwall",
    "license": "MIT",
    "authors": [
        {
            "name": "Del",
            "email": "del@incube8.sg"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/incube8/paymentwall-php"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "paymentwall/paymentwall-php": "dev-master",
        "omnipay/common": "~2.0"
    },
    "require-dev": {
        "omnipay/tests": "~2.0",
        "omnipay/dummy": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\PaymentWall\\": "src/"
        },
        "classmap": [
            "vendor/paymentwall/paymentwall-php/lib/Paymentwall/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
