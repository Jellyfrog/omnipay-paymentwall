{
    "name": "delatbabel/omnipay-paymentwall",
    "type": "library",
    "description": "PaymentWall driver for the Omnipay payment processing library",
    "keywords": [
        "PaymentWall",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/delatbabel/omnipay-paymentwall",
    "license": "MIT",
    "authors": [
        {
            "name": "Del",
            "email": "del@incube8.sg"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\PaymentWall\\" : "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "paymentwall/paymentwall-php": "dev-master",
        "omnipay/common": "~2.0"
    },
    "require-dev": {
        "omnipay/tests": "~2.0",
        "omnipay/dummy": "dev-master"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
